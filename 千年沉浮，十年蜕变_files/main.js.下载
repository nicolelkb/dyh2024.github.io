$(function() {

	OpenAni();

	function OpenAni() {
		setTimeout(function() {
			$(".open_img").addClass("animated fadeOut");
			$("#mainPart").show();
			DyhSwiper("#picSwEcology");
			DyhSwiper("#picSwCulture");
			setTimeout(function() {
				$(".pic_painting").show();
				$("#openScreen").hide();
				setTimeout(function() {
					$(".txt_bub").show();
				}, 500);
			}, 500);
		}, 5000);
	}

	$("#btnSkip").click(function() {
		$(".open_img").addClass("animated fadeOut");
		$("#mainPart").show();
		DyhSwiper("#picSwEcology");
		DyhSwiper("#picSwCulture");
		setTimeout(function() {
			$(".pic_painting").show();
			$("#openScreen").hide();
			setTimeout(function() {
				$(".txt_bub").show();
			}, 500);
		}, 500);
	});


	$(".point_history").click(function() {
		var DataPoint = $(this).attr("data-point");
		console.log(DataPoint);

		$(this).hide();
		$(".point_" + DataPoint + "").show();
	});

	$(".point_place").click(function() {
		var DataPlace = $(this).attr("data-place");
		console.log(DataPlace);
		$(this).hide();
		$("#policy_" + DataPlace + "").show();
	});

	// ★视频
	var dyhVideo = document.getElementById("dyhVideo");
	$("#playBtn").click(function() {
		dyhVideo.play();
		$("#playBtn").hide();
	});

	$("#dyhVideo").click(function() {
		dyhVideo.pause();
		//$("#playBtn").show();
	});

	dyhVideo.addEventListener('ended', function(event) {
		console.log("视频播放结束！");
		$("#playBtn").show();
	});

