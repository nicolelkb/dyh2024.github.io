	$(".dot_city").click(function() {
		$("#maskHumanTrade").show();
		$("#chartBoxHumantrade").show();
		var dataCity = $(this).attr("data-city");
		var dataTitle = $(this).attr("data-title");
		console.log(dataCity);
		console.log(dataTitle);
		$("#titleCity").html(dataTitle);
		EchartHuman(eval("DataChzh" + dataCity + ""), eval("DataHuji" + dataCity + ""));
		EchartTrade(eval("DataTrade" + dataCity + ""));
	});

	$("#btnCloseHumantrade").click(function() {
		$("#maskHumanTrade").hide();
		$("#chartBoxHumantrade").hide();
	});


	EchartHuman(DataChzhBeiJing, DataHujiBeiJing);
	EchartTrade(DataTradeBeiJing);

	// ★ Chart：常住人口VS户籍人口
	function EchartHuman(DataChzhCity, DataHujiCity) {
		var dom = document.getElementById('humanCanal');
		var myChart = echarts.init(dom, null, {
			renderer: 'canvas',
			useDirtyRect: false
		});
		var app = {};
		var option;
		option = {
			//animationDuration: 3000,
			grid: {
				top: 30,
				left: 5,
				right: 10,
				bottom: 0,
				containLabel: true,
			},
			tooltip: {
				trigger: 'axis',
				axisPointer: {
					type: 'shadow'
				},
			},
			legend: {
				data: ['常住人口（万人）', '户籍人口（万人）'],
				itemWidth: 10,
				itemHeight: 10,
				right: 0,
				textStyle: {
					fontSize: 14,
				}
			},
			calculable: true,
			xAxis: [{
				type: 'category',
				axisLine: {
					lineStyle: {
						width: 0.75,
						color: colors0[1],
					}
				},
				axisTick: {
					show: false
				},
				axisLabel: {
					interval: 0,
					rotate: 30,
					fontSize: 14,
				},
				data: ['2005年', '2006年', '2007年', '2008年', '2009年', '2010年', '2011年', '2012年', '2013年', '2014年', '2015年', '2016年', '2017年', '2018年', '2019年', '2020年', '2021年', '2022年', '2023年', ],
			}],
			yAxis: [{
				type: 'value',
				name: "单位（万人）",
				nameTextStyle: {
					color: colors0[0],
					fontSize: 14,
					padding: [0, 0, 0, 110],
				},
				nameGap: 10,
				axisLine: {
					show: true,
					lineStyle: {
						width: 0.75,
						color: colors0[1],
					}
				},
				axisLabel: {
					interval: 0,
					rotate: 30,
					fontSize: 14,
				},
				splitLine: {
					lineStyle: {
						color: colors0[1],
						width: 0.75,
						type: 'dashed',
						opacity: 0.5,
					}
				},
			}],
			series: [{
					name: '常住人口（万人）',
					type: 'bar',
					color: colors1[0],
					barGap: 0,
					data: DataChzhCity,
				},
				{
					name: '户籍人口（万人）',
					type: 'bar',
					color: colors1[1],
					data: DataHujiCity,
				}
			]
		};
		if (option && typeof option === 'object') {
			myChart.setOption(option);
		}
		window.addEventListener('resize', myChart.resize);
	}

	// ★ Chart：进出口总额
	function EchartTrade(DataTradeCity) {
		var dom = document.getElementById('tradeCanal');
		var myChart = echarts.init(dom, null, {
			renderer: 'canvas',
			useDirtyRect: false
		});
		var app = {};
		var option;
		option = {
			//animationDuration: 3000,
			grid: {
				top: 30,
				left: 5,
				right: 10,
				bottom: 0,
				containLabel: true,
			},
			tooltip: {
				trigger: 'axis'
			},
			calculable: true,
			xAxis: {
				type: 'category',
				axisLine: {
					lineStyle: {
						width: 0.75,
						color: colors0[1],
					}
				},
				axisTick: {
					show: false
				},
				axisLabel: {
					interval: 0,
					rotate: 30,
					fontSize: 14,
				},
				splitLine: {
					show: true,
					lineStyle: {
						color: colors0[1],
						width: 0.75,
						type: 'dashed',
						opacity: 0.5,
					}
				},
				data: ['2005年', '2006年', '2007年', '2008年', '2009年', '2010年', '2011年', '2012年', '2013年', '2014年', '2015年', '2016年', '2017年', '2018年', '2019年', '2020年', '2021年', '2022年', '2023年', ],
			},
			yAxis: {
				type: 'value',
				name: "进出口总额（亿元）",
				nameTextStyle: {
					color: colors0[0],
					fontSize: 14,
					padding: [0, 0, 0, 160],
				},
				nameGap: 5,
				axisLine: {
					show: true,
					lineStyle: {
						width: 0.75,
						color: colors0[1],
					}
				},
				splitLine: {
					lineStyle: {
						color: colors0[1],
						width: 0.75,
						type: 'dashed',
						opacity: 0.5,
					}
				},
				axisLabel: {
					interval: 0,
					rotate: 30,
					fontSize: 14,
				},
			},
			series: [{
				data: DataTradeCity,
				type: 'line',
				symbol: 'circle',
				symbolSize: 9,
				itemStyle: {
					color: colors1[0],
					borderColor: '#FFFFFF',
					borderWidth: 2,
				},
				lineStyle: {
					color: colors1[0],
					width: 2,
				},
				areaStyle: {
					color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{
							offset: 0,
							color: colors1[0],
						},
						{
							offset: 1,
							color: colors1[2],
						}
					])
				},
			}]
		};
		if (option && typeof option === 'object') {
			myChart.setOption(option);
		}
		window.addEventListener('resize', myChart.resize);
	}

	// ★ Chart：京杭大运河江苏段2018年货运量
	var dom = document.getElementById('jiangsuTransport');
	var myChart = echarts.init(dom, null, {
		renderer: 'canvas',
		useDirtyRect: false
	});
	var app = {};
	var option;
	option = {
		color: colors2,
		// grid: {
		// 	top: 30,
		// 	left: 5,
		// 	right: 10,
		// 	bottom: 0,
		// 	containLabel: true,
		// },
		tooltip: {
			trigger: 'item',
			textStyle: {
				fontSize: 20,
			}
		},
		legend: {
			itemWidth: 16,
